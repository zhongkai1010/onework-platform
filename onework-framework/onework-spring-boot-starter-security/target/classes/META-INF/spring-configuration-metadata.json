{
  "groups": [
    {
      "name": "onework.security",
      "type": "com.onework.boot.framework.security.config.SecurityProperties",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties"
    }
  ],
  "properties": [
    {
      "name": "onework.security.mock-enable",
      "type": "java.lang.Boolean",
      "description": "mock 模式的开关",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties",
      "defaultValue": false
    },
    {
      "name": "onework.security.mock-secret",
      "type": "java.lang.String",
      "description": "mock 模式的密钥 一定要配置密钥，保证安全性",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties",
      "defaultValue": "test"
    },
    {
      "name": "onework.security.password-encoder-length",
      "type": "java.lang.Integer",
      "description": "PasswordEncoder 加密复杂度，越高开销越大",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties",
      "defaultValue": 4
    },
    {
      "name": "onework.security.permit-all-urls",
      "type": "java.util.List<java.lang.String>",
      "description": "免登录的 URL 列表",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties"
    },
    {
      "name": "onework.security.token-header",
      "type": "java.lang.String",
      "description": "HTTP 请求时，访问令牌的请求 Header",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties",
      "defaultValue": "Authorization"
    },
    {
      "name": "onework.security.token-parameter",
      "type": "java.lang.String",
      "description": "HTTP 请求时，访问令牌的请求参数 <p> 初始目的：解决 WebSocket 无法通过 header 传参，只能通过 token 参数拼接",
      "sourceType": "com.onework.boot.framework.security.config.SecurityProperties",
      "defaultValue": "token"
    }
  ],
  "hints": []
}